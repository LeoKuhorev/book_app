<!DOCTYPE html>
<html lang="en">
<head>
  <% include ../partials/head %>
  <title>Document</title>
</head>
<body>
  <header>
    <% include ../partials/header %>
    <h2>Search Results</h2>
  </header>
  <main>
    <section class="books">
      <% searchArray.forEach( book => { %>
        <div class="book">
          <img src= <%= book.url; %> alt="<%= book.title; %>">
          <h3> <%= book.title; %> </h3>
          <h4> <%= book.author; %> </h4>
          <p><%= book.description; %> </p>
        </div>
      <% }); %>
    </section>
  </main>
  <footer>

  </footer>
</body>
</html>